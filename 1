def bs(li, low, high, val):
    if low <=high:
        mid = (low+high) // 2
        if arr[mid] == val:
            return mid
        elif arr[mid] > val:
            return bs(arr[mid+1:], mid+1, high, val)
        else:
            return bs(arr[:mid-1], low, mid-1, val)
    else:
        return -1

print(bs([1,3,5,7,11,15], 0, 6, 7)) 
